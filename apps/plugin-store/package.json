{
  "name": "plugin-store",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix",
    "lint": "run-s lint:*",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "pinia": "catalog:",
    "vue": "catalog:",
    "vue-router": "catalog:"
  },
  "devDependencies": {
    "@playwright/test": "catalog:playwright",
    "autoprefixer": "catalog:",
    "@tsconfig/node22": "catalog:types",
    "@types/jsdom": "catalog:types",
    "@types/node": "catalog:types",
    "@vitejs/plugin-vue": "catalog:vitejs",
    "@vitejs/plugin-vue-jsx": "catalog:vitejs",
    "@vitest/eslint-plugin": "catalog:vitest",
    "@vue/eslint-config-prettier": "catalog:vue",
    "@vue/eslint-config-typescript": "catalog:vue",
    "@vue/test-utils": "catalog:vue",
    "@vue/tsconfig": "catalog:vue",
    "eslint-plugin-oxlint": "catalog:",
    "eslint-plugin-playwright": "catalog:",
    "eslint-plugin-vue": "catalog:",
    "jiti": "catalog:",
    "jsdom": "catalog:",
    "npm-run-all2": "catalog:",
    "oxlint": "catalog:",
    "tailwindcss": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:",
    "vue-tsc": "catalog:"
  },
  "format": "prettier --write src/"
}
